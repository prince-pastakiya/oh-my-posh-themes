{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#21c7a8",
          "foreground": "#011627",
          "leading_diamond": "\u256d\u2500\ue0b6",
          "options": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\uf179 ",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "ubuntu": "\uf31b ",
            "windows": "\uf327  H4ck3rShell"
          },
          "style": "diamond",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "trailing_diamond": "\ue0b0",
          "type": "os"
        },
        {
          "background": "#00c7fc",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "leading_powerline_symbol": "\ue0d7",
          "style": "powerline",
          "template": " \uec19  CPU: {{ round .PhysicalPercentUsed .Precision }}% | ",
          "type": "sysinfo"
        },
        {
          "background": "#00c7fc",
          "foreground": "#000000",
          "style": "diamond",
          "template": "\uefc5  RAM: {{ (div ((sub .PhysicalTotalMemory .PhysicalAvailableMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }} GB ",
          "trailing_diamond": "\ue0b0",
          "type": "sysinfo"
        },
        {
          "background": "#addb67",
          "foreground": "#011627",
          "leading_diamond": "\ue0d7",
          "options": {
            "time_format": "3:04 PM | Monday"
          },
          "style": "diamond",
          "template": " \ue641 {{ .CurrentDate | date .Format }} ",
          "trailing_diamond": "\ue0b0",
          "type": "time"
        },
        {
          "background": "#2343e2",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0d7",
          "options": {
            "style": "roundrock",
            "threshold": 0
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }} ",
          "trailing_diamond": "\ue0b0",
          "type": "executiontime"
        },
        {
          "type": "docker",
          "background": "#0461ee",
          "powerline_symbol": "\ue0b0",
          "leading_powerline_symbol": "\ue0d7",
          "foreground": "#000000",
          "style": "powerline",
          "options": {
            "display_mode": "context"
          },
          "template": " üê≥ {{ .Context }} | v{{ .Version }} ",
          "trailing_diamond": "\ue0b0"
        },
        {
          "type": "status",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ef5350{{ end }}",
            "{{ if eq .Code 0 }}#22da6e{{ end }}"
          ],
          "style": "plain",
          "options": {
            "always_enabled": true
          },
          "template": " {{ if gt .Code 0 }}‚úò Error{{ else }}‚úî Success{{ end }} "
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#91f2ff",
          "style": "plain",
          "template": "\u256d\u2500",
          "type": "text"
        },
        {
          "background": "#91f2ff",
          "foreground": "#000000",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": " \ue23a  ",
          "trailing_diamond": "ÓÉÄ",
          "type": "shell"
        },
        {
          "options": {
            "root_icon": "\uf292 "
          },
          "style": "diamond",
          "template": " \uf0e7 ",
          "type": "root"
        },
        {
          "background": "#f8677b",
          "foreground": "#000000",
          "leading_diamond": "\ue0b6",
          "options": {
            "folder_icon": "\uf07b ",
            "folder_separator_icon": " \uf061 ",
            "home_icon": "\uf46d",
            "style": "agnoster_full"
          },
          "template": " \ue5ff {{ .Path }} ",
          "style": "diamond",
          "trailing_diamond": "ÓÉÄ",
          "type": "path"
        },
        {
          "background": "#addb67",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#e4cf6a{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f78c6c{{ end }}",
            "{{ if gt .Ahead 0 }}#C792EA{{ end }}",
            "{{ if gt .Behind 0 }}#c792ea{{ end }}"
          ],
          "foreground": "#011627",
          "leading_diamond": "\ue0b6",
          "options": {
            "branch_icon": "",
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "mapped_branches": {
              "feat/*": "üöÄ ",
              "bug/*": "üêõ "
            }
          },
          "style": "diamond",
          "template": " \ue65b GitHub \uf418 {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \ueaff {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "trailing_diamond": "\ue0b4",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "overflow": "break",
      "segments": [
        {
          "type": "python",
          "background": "#306998",
          "foreground": "#FFE873",
          "leading_diamond": "ÓÉÇ",
          "style": "diamond",
          "options": {
            "display_mode": "environment",
            "fetch_virtual_env": true,
            "fetch_version": true
          },
          "template": " \ue606  (.venv) {{ .Venv }} | {{ .Full }} ",
          "trailing_diamond": "ÓÉÄ"
        },
        {
          "background": "#303030",
          "foreground": "#3C873A",
          "leading_diamond": "\ue0b2",
          "options": {
            "fetch_package_manager": true,
            "npm_icon": "<#cc3a3a>\ue71e</> ",
            "yarn_icon": "<#348cba>\ue6a7</> "
          },
          "style": "diamond",
          "template": "\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "trailing_diamond": "\ue0d6",
          "type": "node"
        },
        {
          "type": "node",
          "background": "#303030",
          "foreground": "#FFD43B",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "options": {
            "fetch_package_manager": true,
            "npm_icon": "üì¶ ",
            "yarn_icon": "üß∂ "
          },
          "template": " {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "trailing_diamond": "\ue0b0"
        },
        {
          "type": "java",
          "background": "#e76f00",
          "foreground": "#ffffff",
          "leading_diamond": "ÓÉÇ",
          "style": "diamond",
          "template": " \uef76 | Spark | {{ .Version }} ",
          "trailing_diamond": "ÓÉÄ"
        },
        {
          "type": "terraform",
          "background": "#844fba",
          "foreground": "#ffffff",
          "leading_diamond": "ÓÉÇ",
          "style": "diamond",
          "template": " üß± | Terraform | {{ .Version }} ",
          "trailing_diamond": "ÓÉÄ"
        },
        {
          "type": "helm",
          "background": "#0f1689",
          "foreground": "#ffffff",
          "leading_diamond": "ÓÉÇ",
          "style": "diamond",
          "template": " ‚éà | Helm | {{ .Version }} ",
          "trailing_diamond": "ÓÉÄ"
        },
        {
          "type": "kubectl",
          "background": "#326ce5",
          "foreground": "#ffffff",
          "leading_diamond": "ÓÉÇ",
          "style": "diamond",
          "template": " ‚ò∏ | K8s | {{ .Context }}{{ if .Namespace }}:{{ .Namespace }}{{ end }} ",
          "trailing_diamond": "ÓÉÄ"
        },
        {
          "type": "aws",
          "background": "#ff9900",
          "foreground": "#232f3e",
          "leading_diamond": "ÓÉÇ",
          "style": "diamond",
          "template": " ‚òÅ | AWS | {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} ",
          "trailing_diamond": "ÓÉÄ"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#21c7a8",
          "style": "plain",
          "template": "\u2570\u2500",
          "type": "text"
        },
        {
          "background": "#1DB954",
          "foreground": "#011627",
          "leading_diamond": "\ue0b6",
          "options": {
            "playing_icon": "\uf1bc "
          },
          "style": "diamond",
          "template": " {{ .Icon }}{{ if ne .Status \"stopped\" }}{{ .Artist }} ~ {{ .Track }}{{ end }} ",
          "trailing_diamond": "\ue0c6 ",
          "type": "spotify"
        },
        {
          "foreground": "#22da6e",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ef5350{{ end }}"
          ],
          "options": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\ue285\ue285",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title": true,
  "console_title_style": "template",
  "console_title_template": "{{ .Folder }}",
  "final_space": true,
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#FEF5ED",
    "template": "\ue285 "
  },
  "version": 4
}